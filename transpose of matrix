#include <iostream>
using namespace std;
int main() {
int r,c;
cout<<"ENTER NO OF ROW :";
cin>>r;
cout<<"ENTER NO OF COLLUM :";
cin>>c;
int a[r][c];
int i,j;
cout<<"ENTER MATRIX ELEMENT (row * collum) :";
for(i=0;i<r;i++){
    for(j=0;j<c;j++){
        cin>>a[i][j];
    }
}
cout<<"INPUT MATRIX"<<endl;
for(i=0;i<r;i++){
    for(j=0;j<c;j++){
        cout<<"  "<<a[i][j];
    }
    cout<<endl;
}
cout<<"OUTPUT MATRIX"<<endl;
cout<<"TRANSPOSE OF INPUT MATRIX"<<endl;
 for(i=0;i<c;i++){
     for(j=0;j<r;j++){
         cout<<"  "<<a[j][i];
     }
     cout<<endl;
 }
    return 0;
}
