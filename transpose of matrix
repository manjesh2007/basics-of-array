#include <stdio.h>
int main() {
    int r,c;
    printf("enter the number of row = ");
    scanf("%d",&r);
    printf("enter the number of collum = ");
    scanf("%d",&c);
    int  mk[r][c];
    printf("enter the element\n");
for(int i=0;i<r;i++){
    for(int j=0;j<c;j++){
    scanf("%d",&mk[i][j]);    
    }
    printf("\n");
}
printf(" your matrix\n");
   for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
           printf("%d  ",mk[i][j]);
       }
       printf("\n");
   }
printf(" transpose matrix\n");
   for(int i=0;i<r;i++){
       for(int j=0;j<c;j++){
           printf("%d  ",mk[j][i]);
       }
       printf("\n");
   }
    return 0;
}
